<?php
class API extends CI_Controller {
 
    /**
    * Responsable for auto load the model
    * @return void
    */
    public function __construct()
    {
        parent::__construct();
        $this->load->model('users_model');

        if(!$this->session->userdata('is_logged_in')){
            redirect('admin/login');
        }
    }
}
?>
